{
	"name": "My extension",
	"version": "1.0",
	"description":"Adds ability to hide Tumblr posts, even if they are reblogged.",
	"default_locale": "en",
	"icons": {
		"128":"icon.png"
	},
	"background": {
		"scripts": [ "js/background.js" ]
	},
	"options_page": "options.html",
	"page_action" : {
		"default_icon" : "icon.png"
	},
	"content_scripts": [
		{
			"matches": ["http://www.tumblr.com/dashboard"],
			"css": ["css/content_script.css"],
			"js": ["js/defaults.js", "js/jquery-2.0.3.min.js", "js/content_script.js", "js/hide_formats.js"]
		}
	],
	"web_accessible_resources": [
		"js/jquery-2.0.3.min.map"
	],
	"manifest_version": 2
}